{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "defaultHeaders": {
    "cache-control": "no-cache, no-store, must-revalidate",
    "content-security-policy": "default-src 'self'; script-src 'self' 'unsafe-eval' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; connect-src 'self' https://*.azurestaticapps.net;"
  },
  "mimeTypes": {
    ".dll": "application/octet-stream",
    ".wasm": "application/wasm",
    ".json": "application/json",
    ".dat": "application/octet-stream",
    ".blat": "application/octet-stream",
    ".br": "application/brotli",
    ".gz": "application/gzip"
  }
}
