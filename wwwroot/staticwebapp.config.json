{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif}",
      "/css/*",
      "/js/*",
      "/_framework/*",
      "/*.ico",
      "/*.br",
      "/*.gz"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self'; script-src 'self' 'unsafe-eval' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; font-src 'self'; connect-src 'self' https://*.azurestaticapps.net; worker-src 'self' blob:",
    "cache-control": "no-cache"
  },
  "mimeTypes": {
    ".json": "text/json",
    ".wasm": "application/wasm",
    ".dll": "application/octet-stream",
    ".dat": "application/octet-stream",
    ".blat": "application/octet-stream",
    ".br": "application/brotli",
    ".gz": "application/gzip"
  }
}
