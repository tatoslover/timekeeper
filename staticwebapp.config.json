{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/_framework/*",
      "/css/*",
      "/js/*",
      "/images/*",
      "/*.ico",
      "/*.png"
    ]
  },
  "routes": [
    {
      "route": "/_framework/blazor.webassembly.js",
      "headers": {
        "content-type": "application/javascript"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self'; script-src 'self' 'unsafe-eval' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; font-src 'self'; connect-src 'self' https://*.azurestaticapps.net; worker-src 'self' blob:;",
    "Cache-Control": "no-cache"
  },
  "mimeTypes": {
    ".html": "text/html",
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css",
    ".wasm": "application/wasm",
    ".dll": "application/octet-stream",
    ".dat": "application/octet-stream",
    ".blat": "application/octet-stream"
  }
}
