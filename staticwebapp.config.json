{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/_framework/*",
      "/_content/*",
      "/css/*",
      "/js/*",
      "/images/*",
      "/*.ico",
      "/*.png",
      "/*.svg"
    ]
  },
  "routes": [
    {
      "route": "/_framework/*",
      "headers": {
        "cache-control": "public, max-age=604800, immutable"
      }
    },
    {
      "route": "/_framework/blazor.webassembly.js",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "public, max-age=604800, immutable"
      }
    },
    {
      "route": "/_framework/*.js",
      "headers": {
        "content-type": "application/javascript"
      }
    },
    {
      "route": "/_framework/*.wasm",
      "headers": {
        "content-type": "application/wasm"
      }
    },
    {
      "route": "/_framework/*.dll",
      "headers": {
        "content-type": "application/octet-stream"
      }
    },
    {
      "route": "/_framework/*.dat",
      "headers": {
        "content-type": "application/octet-stream"
      }
    },
    {
      "route": "/_framework/*.blat",
      "headers": {
        "content-type": "application/octet-stream"
      }
    },
    {
      "route": "/_framework/*.gz",
      "headers": {
        "content-type": "application/gzip"
      }
    },
    {
      "route": "/_framework/*.br",
      "headers": {
        "content-type": "application/brotli"
      }
    },
    {
      "route": "/css/*.css",
      "headers": {
        "content-type": "text/css"
      }
    },
    {
      "route": "/js/*.js",
      "headers": {
        "content-type": "application/javascript"
      }
    },
    {
      "route": "/*",
      "methods": ["GET"],
      "headers": {
        "cache-control": "no-cache"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-eval' https://green-water-06a334700.2.azurestaticapps.net; script-src 'self' 'unsafe-eval' 'unsafe-inline' https://green-water-06a334700.2.azurestaticapps.net; style-src 'self' 'unsafe-inline'; img-src 'self' data:; font-src 'self'; connect-src 'self' https://*.azurestaticapps.net; worker-src 'self' blob:; frame-ancestors 'self'; base-uri 'self';",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block"
  },
  "mimeTypes": {
    ".html": "text/html",
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css",
    ".wasm": "application/wasm",
    ".dll": "application/octet-stream",
    ".dat": "application/octet-stream",
    ".blat": "application/octet-stream",
    ".br": "application/brotli",
    ".gz": "application/gzip",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".ico": "image/x-icon"
  }
}
